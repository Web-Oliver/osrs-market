# OSRS Market Frontend - Environment Configuration
# Copy this file to .env and configure your settings

# API Endpoints
VITE_NODE_API_URL=http://localhost:3001
VITE_OSRS_WIKI_API_URL=https://prices.runescape.wiki/api/v1/osrs

# Development Settings
VITE_APP_ENV=development
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_AUTO_TRAINING=true

# Monitoring & Analytics
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_LOG_LEVEL=info

# UI Configuration
VITE_DEFAULT_REFRESH_INTERVAL=30000
VITE_MAX_CHART_DATA_POINTS=1000
VITE_ENABLE_REAL_TIME_UPDATES=true